<h2 class="header">Editing Checklist
  <span class="spanEdit"><%= link_to 'Back', @checklist %></span>
</h2>

<div class='container'>
    <div class='row'>
        <div class='col s4'>
            <%= render 'form', checklist: @checklist %>
        </div>

        <div class='col s8'>
            <div id="map3"></div>
        </div>
    </div>
</div>

<script type="text/javascript">
    setLatLng(<%= @lat %>, <%= @lng %>);
    if (window.google) {
        initMap2();
    } else {
        $.ajax('<%= @srctext %>', {
            crossDomain: true,
            dataType: 'script'
        }).done(function (response) {
            initMap2();
        }).fail(function () {
            console.log('Ajax fail for initMap2');
        });
    }
</script>
